  "dns": { 
    "servers": [
    {
      "tag": "dns_proxy",
      "address": "tls://1.0.0.1:853",
      "strategy": "prefer_ipv4",
      "address_resolver": "dns_resolver"
    }, {
      "tag": "dns_direct",
      "address": "https://223.5.5.5/dns-query",
      "strategy": "prefer_ipv4",
      "address_resolver": "dns_resolver",
      "detour": "DIRECT"
    }, {
      "tag": "dns_fakeip",
      "address": "fakeip"
