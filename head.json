      "detour": "DIRECT"
    }, {
      "tag": "block",
      "address": "rcode://success"
    }
    ],
    "rules": [
    {
      "outbound": ["any"],
      "server": "dns_resolver"
    }, {
      "geosite": ["cn"],
	  "query_type": [ "A", "AAAA" ],
      "server": "dns_direct"
	}, {
