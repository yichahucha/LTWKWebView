	  "query_type": [ "A", "AAAA" ],
      "server": "dns_fakeip"
    },
    { 
      "rule-set": [ "geosite-cn" ],
      "query_type": [ "A", "AAAA" ],
      "server": "dns_direct"
    }
    ],
    "final": "dns_direct",
    "independent_cache": true,
    "reverse_mapping": true,
    "fakeip": { "enabled": true, "inet4_range": "198.18.0.0/16", "inet6_range": "fc00::/16" }
